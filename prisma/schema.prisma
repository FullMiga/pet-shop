generator client {
  provider = "prisma-client-js"
  output   = "../app/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Appointment {
  id          String   @id @default(uuid())
  tutorName   String
  petName     String
  phone       String
  description String
  schedule    DateTime

  @@map("appointments")
}
